{
    "version": "1",
    "manifest_version": 2,
    "description": "this a tool to scrap data from dmade by sarraj.marwen@gmail.com ",
    "name": "wanneroobusiness scrapper",
    "icons": {
        "128": "images/off.png",
        "48":  "images/on48.png",
        "16":  "images/on16.png"
    },
    "permissions": [
        "proxy",
        "tabs",
        "activeTab",
        "webNavigation",
        "https://www.wanneroobusiness.com/*"
    ],
    "background": {
    "persistent": true,
    "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "images/off.png",
        "default_popup": ""
    }
}

